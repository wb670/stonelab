		<!-- pcc依赖 -->
		<dependency>
			<groupId>com.alibaba.pc2</groupId>
			<artifactId>pc2.common</artifactId>
			<version>1.1.0-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>com.alibaba.pc2</groupId>
			<artifactId>pc2.client.agent</artifactId>
