<?xml version="1.0" encoding="GB18030"?>
<!DOCTYPE rialto SYSTEM "rialto.dtd">
<rialto>
    <pipeline name="vasMessage" poolSize="10">
        <entry name="entry">
            <listener name="vasMessageListener" ref="vasMessageListener" enable="true" />
        </entry>
        <modules>
            <module name="vasMessageModule">
                <action name="vasAction" ref="vasAction">
                    <pre-injections>
                        <injection to="message" />
                    </pre-injections>
                </action>
            </module>
        </modules>
    </pipeline>
</rialto>